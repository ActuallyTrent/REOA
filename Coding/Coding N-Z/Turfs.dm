	Block_Team
		Enter(atom/movable/a)
			if(istype(a,/mob/player/client))
				var/mob/player/client/m = a
				if(m.team)
					return 0
				else
					return..()
			return..()
	Block_Zone
		Enter(atom/movable/a)
			if(istype(a,/mob))
				var/mob/M=a
				if(M.alignment)
					return..()
			else if(istype(a,/obj/effects/Laser_Wire)||istype(a,/obj/effects/Grenade)||istype(a,/obj/projectiles)||istype(a,/obj/effects/bullet))
				return ..()
			return 0
